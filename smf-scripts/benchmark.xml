<bench-config>
   <master
         bindAddress="${127.0.0.1:master.address}"
         port="${21032:master.port}"/>
   <benchmark
         initSize="${NODES}"
         maxSize="${NODES}"
         increment="1">
      <DestroyWrapper
            runOnAllSlaves="true"/>
      <StartCluster
            staggerSlaveStartup="true"
            delayAfterFirstSlaveStarts="5000"
            delayBetweenStartingSlaves="1000"/>
      <ClusterValidation
            partialReplication="false"/>
      <InitStressTestWarmup entrySize="10" opsCountStatusLog="100000" numThreads="4" transactionSize="50" duration="60000"/>
      <ContinueStressTestWarmup entrySize="10" opsCountStatusLog="100000" numThreads="4" transactionSize="50" duration="60000"/>
      <ClearCluster/>
      <InitStressTest entrySize="10" numEntries="1000" opsCountStatusLog="500000" numThreads="4" useTransactions="true" transactionSize="1000" writePercentage="5" writeTxPercentage="1" duration="60000" />
      <ContinueStressTest entrySize="10" numEntries="1000" opsCountStatusLog="500000" numThreads="4" useTransactions="true" transactionSize="1000" writePercentage="5" writeTxPercentage="1" duration="60000" />
      <CsvReportGeneration fileName="fflf-tdmock-g2800ns-p67000ns-c64000ns" />
   </benchmark>
   <products>
   <!-- put here the contents of the product file fflf-tdmock-g2800ns-p67000ns-c64000ns.xml -->
   <!-- local dir: /home/ubuntu/radargun/smf-scripts -->
   <!-- RADARGUN_DIR:  -->
   </products>
   <reports>
      <report name="All" includeAll="true" />
   </reports>
</bench-config>
